<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:ClipThief.Ui.ViewModels"
                    xmlns:views="clr-namespace:ClipThief.Ui.Views">
    <DataTemplate DataType="{x:Type viewModels:VideoFormatSelectionViewModel}">
        <views:VideoFormatSelectionView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:DownloadViewModel}">
        <views:DownloadView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:VideoCuttingViewModel}">
        <views:VideoCuttingView />
    </DataTemplate>
</ResourceDictionary>